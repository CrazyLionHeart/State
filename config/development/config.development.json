{
    "mongodb": {
        "connectTimeout": 300000,
        "host": [
            "127.0.0.1"
        ],
        "journal": true,
        "replicaSet": "babypages",
        "writeConcern": 2,
        "database": "grendizer_dev",
        "max_autoreconnect": 5
    },
    "loggingconfig": {
        "disable_existing_loggers": false,
        "formatters": {
            "simple": {
                "datefmt": "%y %b %d, %H:%M:%S",
                "format": "%(levelname)s:%(name)s: %(message)s \r\n(%(asctime)s; %(filename)s:%(lineno)d)\r\n"
            }
        },
        "handlers": {
            "console": {
                "class": "logging.StreamHandler",
                "formatter": "simple",
                "level": "DEBUG",
                "stream": "ext://sys.stdout"
            },
            "sentry": {
                "class": "raven.handlers.logging.SentryHandler",
                "dsn": "http://ab22cadfc2bc47a1a50512e71c57b4ac:b0e930cbf76a4226a336b0b7bca534aa@sentry.bbp/8",
                "level": "WARNING"
            }
        },
        "root": {
            "level": "DEBUG",
            "handlers": [
                "console"
            ]
        },
        "version": 1
    },
    "Raven": {
        "public": "ab22cadfc2bc47a1a50512e71c57b4ac",
        "private": "b0e930cbf76a4226a336b0b7bca534aa",
        "host": "sentry.bbp/8"
    }
}
